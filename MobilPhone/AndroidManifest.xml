<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="at.gaeckler.scheduler.android" android:versionName="1.0.4" android:versionCode="4">
    <uses-sdk android:minSdkVersion="7" />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application android:label="@string/app_name" android:icon="@drawable/scheduler">
        <activity android:name=".MobileForm"
                  android:label="@string/app_name" android:icon="@drawable/scheduler">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Settings" android:icon="@drawable/scheduler"></activity>

    </application>
</manifest>